    "connect-history-api-fallback": "^1.6.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
